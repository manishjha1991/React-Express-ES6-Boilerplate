  "dependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-builtin-extend": "^1.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.6.0",
    "csv-parse": "^4.3.0",
    "lodash": "^4.17.4",
    "mongoose": "^5.0.0-rc2",
    "uuid": "^3.1.0",
    "validator": "^9.2.0"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "babel-jest": "^21.2.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "jest": "^21.2.1"
